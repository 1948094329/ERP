        <parent></parent>
        <icon>el-icon-menu</icon>
        <group>stockman,stockmanager</group>
        <path>stock</path>
        <componentpath>admin/global/Breadcrumb</componentpath>
        <meta></meta>
        <viewtype></viewtype>
        <isbackground>true</isbackground>
